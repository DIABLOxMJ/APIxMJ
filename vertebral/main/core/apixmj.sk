function mj_core_chat(player: player, message: text):
	mj_editfiles_boolean("userdata.%{_player}'s uuid%", "data.temp.mute", "modify", false)
	if {_player} is op:
		send formatted "%{_message}%" to all players
	else:
		send "%{_message}%" to all players
	stop

	trigger:
		if player has permission "*" or "mj.*" or "mj.report.*" or "mj.report.admin":
			if arg-1 is set:
				if arg-1 is "remove":
					if arg-2 is set:
						mj_editfiles("core.data", "data.report.%arg-2%", "delete")
						mj_message(player, "core", "data.report.prefix", "core", "data.report.remove", arg-2)
					else:

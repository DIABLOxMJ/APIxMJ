function mj_core_menu_recipes(player: player):
	set {_title} to mj_editfiles_gettext("core", "data.recipes.title.viewer", {_player}, "")
	mj_gui_generator({_player}, 6, true, "default", {_title}, "recipes")
	set {_slot} to 0

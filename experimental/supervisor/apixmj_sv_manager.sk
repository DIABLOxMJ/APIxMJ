function mj_manager_help(player: player, id: text):
	set {_lang} to mj_userdata_language({_player})
	set {_name} to mj_editfiles("supervisor.help.%{_lang}%", "data.%{_id}%.name")
	if {_name} is set:
		set {_border} to mj_editfiles_gettext("supervisor", "data.help.border", {_player}, {_name})
		send "%{_border}%" to {_player}
		mj_message_simple({_player}, "supervisor", "data.help.description")
		set {_list.1::*} to mj_nodes("supervisor.help.%{_lang}%", "data.%{_id}%.description")
		loop {_list.1::*}:
			set {_send} to mj_editfiles("supervisor.help.%{_lang}%", "data.%{_id}%.description.%loop-value%")
			send "%{_send}%" to {_player}
		send "&0" to {_player}
		mj_message_simple({_player}, "supervisor", "data.help.info")
		set {_list.2::*} to mj_nodes("supervisor.help.%{_lang}%", "data.%{_id}%.info")
		loop {_list.2::*}:
			set {_send} to mj_editfiles("supervisor.help.%{_lang}%", "data.%{_id}%.info.%loop-value%")
			send "%{_send}%" to {_player}
		send "%{_border}%" to {_player}
	else:
		mj_message({_player}, "supervisor", "data.prefix", "supervisor", "data.help.argument")
	stop

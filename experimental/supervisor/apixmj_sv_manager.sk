function mj_manager_update_data():
	set {_link} to mj_editfiles("github")
	delete file "plugins/APIxMJ/manager/data.yml"
	delete file "plugins/APIxMJ/manager/language/fr.yml"
	delete file "plugins/APIxMJ/manager/help/fr.yml"
	wait 2 ticks
	download file from "%{_link}%/main/experimental/manager/data.yml" to file "plugins/APIxMJ/manager/data.yml"
	download file from "%{_link}%/main/experimental/manager/language/fr.yml" to file "plugins/APIxMJ/manager/language/fr.yml"
	download file from "%{_link}%/main/experimental/manager/help/fr.yml" to file "plugins/APIxMJ/manager/help/fr.yml"
	wait 1 second
	mj_files_sv_core()
	send "[APIxMJ] Database for Manager Updated !" to console
	stop

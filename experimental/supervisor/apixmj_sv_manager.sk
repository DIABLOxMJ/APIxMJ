# =====================================================================================================================
# APIxMJ - Files
# ---------------------------------------------------------------------------------------------------------------------
on load:
	set {_list::*} to mj_manager_skript_list()
	if {_list::*} contains "apixmj_sv_core.sk":
		send "[APIxMJ] Loading supervisor_manager…" to console
	else:
		broadcast "[APIxMJ] Script loading cancel It requires (apixmj_sv_core.sk)"
		unload script file "apixmj_sv_manager.sk"
	stop
# ---------------------------------------------------------------------------------------------------------------------
on skript stop:
	rename file "plugins/Skript/scripts/apixmj_sv_manager.sk" to "-apixmj_sv_manager.sk"
	send "[APIxMJ] Unloading supervisor_manager…"
	stop
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Files
# =====================================================================================================================
# APIxMJ - Command
# ---------------------------------------------------------------------------------------------------------------------
command /manager [<text>] [<text>]:
	executable by: player
	trigger:
		if player is op:
			if arg-1 is set:
				if arg-1 is "reload":
					if arg-2 is "files":
						mj_files_sv_core()
						send "[APIxMJ] Data reloaded"
					else:
						unload script file "apixmj_sv_manager.sk"
						player command "/sk reload apixmj_sv_core"
				else if arg-1 is "data_update":
					mj_manager_update_data()
					mj_message(player, "supervisor", "data.prefix", "supervisor", "data.data_update")
			else:
				mj_manager_menu(player)
		stop
# ---------------------------------------------------------------------------------------------------------------------
on tab complete of "/manager":
	set tab completions for position 1 to "reload" and "data_update"
	if tab arg-1 is "reload":
		set tab completions for position 2 to "files"
	stop
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Command
# =====================================================================================================================
# APIxMJ - Gui Function
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_return(player: player, id: text="main", slot: number=0):
	set {_return} to mj_editfiles_gettext("supervisor", "data.button.return", {_player})
	make a gui slot {_slot} of {_player} with barrier named {_return} to run:
		mj_manager_menu({_player}, {_id})
		stop
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_experimental(player: player, source: text, id: text) :: text:
	set {_value} to mj_editfiles_gettext("supervisor", "data.experimental.stable", {_player})
	set {_check} to mj_editfiles_boolean("supervisor.data", "data.addon.%{_source}%.%{_id}%.experimental")
	if {_check} is true:
		set {_value} to mj_editfiles_gettext("supervisor", "data.experimental.instable", {_player})
	return {_value}
# ---------------------------------------------------------------------------------------------------------------------
on load:
	set {_list.1::*} to mj_nodes("supervisor.data", "data.addon")
	loop {_list.1::*}:
		set {_value.1} to loop-value-1
		set {_list.2::*} to mj_nodes("supervisor.data", "data.addon.%{_value.1}%")
		loop {_list.2::*}:
			set {_value.2} to loop-value-2
			set {_check} to yaml value "data.manager.addon.%{_value.1}%.%{_value.2}%.installed" from "mj.temp"
			if {_check} isn't set:
				mj_editfiles("temp", "data.manager.addon.%{_value.1}%.%{_value.2}%.version", "modify", "0000.00.00.0000")
				mj_editfiles_boolean("temp", "data.manager.addon.%{_value.1}%.%{_value.2}%.installed", "modify", false)
				mj_editfiles_boolean("temp", "data.manager.addon.%{_value.1}%.%{_value.2}%.condition", "modify", false)
				if {_value.2} is "supervisor":
					mj_editfiles_boolean("temp", "data.manager.addon.%{_value.1}%.%{_value.2}%.installed", "modify", true)
					mj_editfiles_boolean("temp", "data.manager.addon.%{_value.1}%.%{_value.2}%.condition", "modify", true)
	stop
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Gui Function
# =====================================================================================================================
# APIxMJ - Gui Request
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_request(player: player, source: text, id: text, option: text):
	set {_title} to mj_editfiles_gettext("supervisor", "data.menu.confirm")
	mj_gui_generator({_player}, 3, false, "default", {_title}, "none", light gray glass pane)
	mj_manager_menu_request_button({_player}, {_source}, {_id}, "confirm", {_option})
	mj_manager_menu_request_button({_player}, {_source}, {_id}, "cancel", {_option})
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_request_button(player: player, source: text, id: text, type: text, option: text):
	set {_name} to mj_item_name({_player}, "supervisor.data", "data.button.request.%{_type}%.name")
	set {_slot} to mj_editfiles_number("supervisor.data", "data.button.request.%{_type}%.slot")
	set {_item.1} to mj_item_convert("supervisor.data", "data.button.request.%{_type}%.item")
	set {_item.2} to mj_item_description({_player}, {_item.1}, "data.button.request.%{_type}%")
	set {_item.3} to mj_item_button({_player}, {_item.2}, "left_click", "supervisor", "data.open.choice")
	make a gui slot {_slot} of {_player} with {_item.3} named {_name} to close:
		if {_type} is "confirm":
			if {_option} is "install" or "update":
				mj_manager_menu_control_installer({_player}, {_source}, {_id}, {_option})
			else if {_option} is "reinstall":
				mj_manager_menu_control_reinstaller({_player}, {_source}, {_id})
			else if {_option} is "uninstall":
				mj_manager_menu_control_uninstaller({_player}, {_source}, {_id})
			else:
				mj_manager_menu_control({_player}, {_source}, {_id})
		else:
			mj_manager_menu_control({_player}, {_source}, {_id})
		stop
	stop
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Gui Request
# =====================================================================================================================
# APIxMJ - Gui Main
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu(player: player, id: text="main"):
	if {_id} is "main":
		set {_title} to mj_editfiles_gettext("supervisor", "data.menu.main", {_player})
		set {_item} to mj_item_convert("supervisor.config", "data.gui_background.item")
	else:
		set {_name} to mj_editfiles_gettext("supervisor", "data.index.%{_id}%.name", {_player})
		set {_title} to mj_editfiles_gettext("supervisor", "data.menu.menu", {_player}, {_name})
		if length of {_name} >= 16:
			set {_name} to mj_editfiles_gettext("supervisor", "data.index.%{_id}%.name_lite", {_player})
			set {_title} to mj_editfiles_gettext("supervisor", "data.menu.menu_lite", {_player}, {_name})
		set {_item} to mj_item_convert("supervisor.data", "data.index.%{_id}%.gui_background.item")
	mj_gui_generator({_player}, 6, false, "default", {_title}, "none", {_item})
	mj_manager_menu_index({_player}, {_id})
	mj_manager_menu_addon({_player}, {_id})
	if {_id} isn't "main":
		mj_manager_menu_control_button_condition_list({_player}, {_id})
		set {_return} to mj_editfiles("supervisor.data", "data.index.%{_id}%.menu_back")
		mj_manager_menu_return({_player}, {_return}, 49)
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_index(player: player, id: text):
	set {_list::*} to mj_nodes("supervisor.data", "data.index")
	loop {_list::*}:
		set {_value} to mj_editfiles("supervisor.data", "data.index.%loop-value%.menu_back")
		if {_id} is {_value}:
			mj_manager_menu_index_button_generator({_player}, loop-value)
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_addon(player: player, id: text):
	set {_list::*} to mj_nodes("supervisor.data", "data.addon.%{_id}%")
	loop {_list::*}:
		mj_manager_menu_addon_button_generator({_player}, {_id}, loop-value)
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_index_button_generator(player: player, id: text):
	set {_visible} to mj_editfiles_boolean("supervisor.data", "data.index.%{_id}%.visible")
	if {_visible} is true:
		set {_name} to mj_editfiles_gettext("supervisor", "data.index.%{_id}%.name", {_player})
		set {_title} to mj_editfiles_gettext("supervisor", "data.menu.name", {_player}, {_name})
		set {_slot} to mj_editfiles_number("supervisor.data", "data.index.%{_id}%.slot")
		set {_item.1} to mj_item_convert("supervisor.data", "data.index.%{_id}%.item", true)
		set {_item.2} to mj_item_description({_player}, {_item.1}, "data.index.%{_id}%")
		set {_item.3} to mj_item_button({_player}, {_item.2}, "left_click", "supervisor", "data.open.gui_folder")
		make a gui slot {_slot} of {_player} with {_item.3} named {_title} to run:
			set {_next} to mj_editfiles("supervisor.data", "data.index.%{_id}%.menu_next")
			if {_next} isn't "none":
				mj_manager_menu({_player}, {_next})
			stop
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_addon_button_generator(player: player, source: text, id: text):
	set {_visible} to mj_editfiles_boolean("supervisor.data", "data.addon.%{_source}%.%{_id}%.visible")
	if {_visible} is true:
		set {_name} to mj_editfiles_gettext("supervisor", "data.addon.%{_source}%.%{_id}%.name", {_player})
		set {_title} to mj_editfiles_gettext("supervisor", "data.menu.name", {_player}, {_name})
		set {_exp} to mj_manager_menu_experimental({_player}, {_source}, {_id})
		set {_slot} to mj_editfiles_number("supervisor.data", "data.addon.%{_source}%.%{_id}%.slot")
		set {_item.1} to mj_item_convert("supervisor.data", "data.addon.%{_source}%.%{_id}%.item", true)
		set {_item.2} to mj_item_description({_player}, {_item.1}, "data.addon.%{_source}%.%{_id}%")
		set {_item.3} to mj_item_button({_player}, {_item.2}, "left_click", "supervisor", "data.open.gui_panel")
		make a gui slot {_slot} of {_player} with {_item.3} named "%{_title}% %{_exp}%" to run:
			mj_manager_menu_control({_player}, {_source}, {_id})
			stop
	stop
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Gui Main
# =====================================================================================================================
# APIxMJ - Gui Control
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control(player: player, source: text, id: text):
	set {_name} to mj_editfiles_gettext("supervisor", "data.addon.%{_source}%.%{_id}%.name", {_player})
	set {_title} to mj_editfiles_gettext("supervisor", "data.menu.manager", {_player}, {_name})
	if length of {_name} >= 16:
		set {_name} to mj_editfiles_gettext("supervisor", "data.addon.%{_source}%.%{_id}%.name_lite", {_player})
		set {_title} to mj_editfiles_gettext("supervisor", "data.menu.manager_lite", {_player}, {_name})
	set {_item} to mj_item_convert("supervisor.data", "data.addon.%{_source}%.%{_id}%.gui_background.item")
	mj_gui_generator({_player}, 5, false, "default", {_title}, "none", {_item})
	mj_manager_menu_control_button_status({_player}, {_source}, {_id}, 10)
	mj_manager_menu_control_button_installer({_player}, {_source}, {_id})
	mj_manager_menu_control_button_update({_player}, {_source}, {_id})
	mj_manager_menu_control_button_help({_player}, {_source}, {_id})
	if {_id} isn't "supervisor":
		mj_manager_menu_control_button_condition({_player}, {_source}, {_id})
		mj_manager_menu_control_button_uninstaller({_player}, {_source}, {_id})
		mj_manager_menu_control_button_reinstaller({_player}, {_source}, {_id})
	mj_manager_menu_return({_player}, {_source}, 40)
	stop
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Gui Control
# =====================================================================================================================
# APIxMJ - Gui Control (Status)
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control_button_status(player: player, source: text, id: text, slot: number):
	set {_name} to mj_editfiles_gettext("supervisor", "data.addon.%{_source}%.%{_id}%.name", {_player})
	set {_exp} to mj_manager_menu_experimental({_player}, {_source}, {_id})
	set {_item.1} to paper
	set {_desc} to mj_editfiles_gettext("supervisor", "data.category.info", {_player})
	set line 2 of lore of {_item.1} to "%{_desc}%"
	set {_item.2} to mj_manager_menu_control_button_status_data({_player}, {_source}, {_id}, {_item.1}, "version")
	set {_item.3} to mj_manager_menu_control_button_status_data({_player}, {_source}, {_id}, {_item.2}, "depends")
	set {_item.4} to mj_manager_menu_control_button_status_data({_player}, {_source}, {_id}, {_item.3}, "skript")
	set {_item.5} to mj_manager_menu_control_button_status_data({_player}, {_source}, {_id}, {_item.4}, "file")
	set {_item.6} to mj_item_description({_player}, {_item.5}, "data.addon.%{_source}%.%{_id}%")	
	make a gui slot {_slot} of {_player} with {_item.6} named "%{_name}% %{_exp}%" to run:
		stop
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control_button_status_data(player: player, source: text, id: text, item: item, type: text) :: item:
	set {_count} to mj_item_lore_count({_item})
	if {_type} is "version":
		set {_data.1} to mj_editfiles("temp", "data.manager.addon.%{_source}%.%{_id}%.%{_type}%")
		set {_data.2} to mj_editfiles_gettext("supervisor", "data.category.%{_type}%.1", {_player}, {_data.1})
		set line {_count} of lore of {_item} to "%{_data.2}%"
		add 2 to {_count}
	else:
		set {_data.1} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.%{_type}%.1")
		if {_data.1} isn't "none":
			set {_data.2} to mj_editfiles_gettext("supervisor", "data.category.%{_type}%", {_player})
			set line {_count} of lore of {_item} to "%{_data.2}%"
			add 1 to {_count}
			set {_list::*} to mj_nodes("supervisor.data", "data.addon.%{_source}%.%{_id}%.%{_type}%")
			loop {_list::*}:
				set {_data.3} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.%{_type}%.%loop-value%")
				set {_data.4} to mj_editfiles_gettext("supervisor", "data.button.argument", {_player}, {_data.3})
				if {_type} is "depends":
					set {_name.1} to mj_editfiles_gettext("supervisor", "data.addon.%{_data.3}%.name", {_player})
					set {_name.2} to uncolored {_name.1}
					set {_data.4} to mj_editfiles_gettext("supervisor", "data.button.argument", {_player}, "&a%{_name.2}%")
				set line {_count} of lore of {_item} to "%{_data.4}%"
				add 1 to {_count}
	return {_item}
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Gui Control (Status)
# =====================================================================================================================
# APIxMJ - Gui Control (Installer)
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control_button_installer(player: player, source: text, id: text):
	set {_name} to mj_editfiles_gettext("supervisor", "data.button.installer.name", {_player})
	set {_slot} to mj_editfiles_number("supervisor.data", "data.button.installer.slot")
	set {_installed} to mj_editfiles_boolean("temp", "data.manager.addon.%{_source}%.%{_id}%.installed")
	set {_item} to minecart
	set {_mode} to mj_editfiles_gettext("supervisor", "data.type.not_installed", {_player})
	if {_installed} is true:
		set {_item} to minecart with chest
		set {_mode} to mj_editfiles_gettext("supervisor", "data.type.installed", {_player})
	else:
		set {_version} to mj_editfiles("temp", "data.manager.addon.%{_source}%.%{_id}%.version")
		if {_version} isn't "0000.00.00.0000":
			set {_mode} to mj_editfiles_gettext("supervisor", "data.type.uninstalled", {_player})
	set {_value.1} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.depends.1")
	if {_value.1} isn't "none":
		set {_list::*} to mj_nodes("supervisor.data", "data.addon.%{_source}%.%{_id}%.depends")
		loop {_list::*}:
			set {_value.1} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.depends.%loop-value%")
			set {_value.2} to mj_editfiles_boolean("temp", "data.manager.addon.%{_value.1}%.installed")
			set {_value.3} to mj_editfiles_gettext("supervisor", "data.addon.%{_value.1}%.name", {_player})
			if {_value.2} isn't true:
				set {_name} to mj_editfiles_gettext("supervisor", "data.button.installer.request", {_player}, {_value.3})
				set {_installed} to true
				exit loop
	set {_install} to mj_editfiles_gettext("supervisor", "data.category.type", {_player}, {_mode})
	set line 2 of lore of {_item} to "%{_install}%"
	set {_item} to mj_item_description({_player}, {_item}, "data.button.installer")
	if {_installed} isn't true:
		set {_item} to mj_item_button({_player}, {_item}, "left_click", "supervisor", "data.open.install")
	make a gui slot {_slot} of {_player} with {_item} named {_name} to run:
		if {_installed} isn't true:
			mj_manager_menu_request({_player}, {_source}, {_id}, "install")
		stop
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control_installer(player: player, source: text, id: text, option: text):
	set {_name} to mj_editfiles_gettext("supervisor", "data.addon.%{_source}%.%{_id}%.name", {_player})
	set {_github} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.github_folder")
	set {_folder} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.folder")
	set {_link} to mj_editfiles("github")
	create script "plugins/APIxMJ/%{_folder}%/language/log.txt"
	delete file "plugins/APIxMJ/%{_folder}%/language/log.txt"
	create script "plugins/APIxMJ/%{_folder}%/help/log.txt"
	delete file "plugins/APIxMJ/%{_folder}%/help/log.txt"
	set {_version} to text from "%{_link}%/main/experimental/%{_github}%/version.txt"
	mj_editfiles("temp", "data.manager.addon.%{_source}%.%{_id}%.version", "modify", {_version})
	mj_editfiles_boolean("temp", "data.manager.addon.%{_source}%.%{_id}%.installed", "modify", true)
	mj_editfiles_boolean("temp", "data.manager.addon.%{_source}%.%{_id}%.condition", "modify", true)
	mj_manager_menu_control_installer_skript({_player}, {_source}, {_id}, {_option})
	if {_option} is "install":	
		mj_message({_player}, "supervisor", "data.prefix", "supervisor", "data.process.installed", {_name})
	else if {_option} is "reinstall":
		mj_message({_player}, "supervisor", "data.prefix", "supervisor", "data.process.reinstalled", {_name})
	else if {_option} is "update":
		mj_message({_player}, "supervisor", "data.prefix", "supervisor", "data.process.updated", {_name})
	mj_manager_menu_control({_player}, {_source}, {_id})
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control_installer_skript(player: player, source: text, id: text, option: text):
	set {_link} to mj_editfiles("github")
	set {_github} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.github_folder")
	set {_list::*} to mj_nodes("supervisor.data", "data.addon.%{_source}%.%{_id}%.skript")
	loop {_list::*}:
		set {_skript} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.skript.%loop-value%")
		set {_list::*} to mj_manager_skript_list()
		if {_list::*} contains {_skript}:
			delete file "plugins/Skript/scripts/%{_skript}%.sk"
			wait 2 ticks
		download file from "%{_link}%/main/experimental/%{_github}%/%{_skript}%.sk" to file "plugins/Skript/scripts/%{_skript}%.sk"
	if {_option} is "install" or "reinstall":	
		mj_manager_menu_control_installer_file({_player}, {_source}, {_id})
	wait 2 seconds
	set {_start} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.skript.1")
	reload script "%{_start}%"
	stop
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control_installer_file(player: player, source: text, id: text):
	set {_link} to mj_editfiles("github")
	set {_github} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.github_folder")
	set {_folder} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.folder")
	set {_list.1::*} to mj_nodes("supervisor.data", "data.addon.%{_source}%.%{_id}%.yml")
	loop {_list.1::*}:
		set {_file} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.yml.%loop-value%")
		if {_file} isn't "none":
			download file from "%{_link}%/main/experimental/%{_github}%/%{_file}%.yml" to file "plugins/APIxMJ/%{_folder}%/%{_file}%.yml"
	set {_list.2::*} to mj_nodes("supervisor.data", "data.addon.%{_source}%.%{_id}%.preset")
	loop {_list.2::*}:
		set {_file} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.preset.%loop-value%")
		if {_file} isn't "none":
			create script "plugins/APIxMJ/%{_folder}%/preset/log.txt"
			delete file "plugins/APIxMJ/%{_folder}%/preset/log.txt"
			wait 2 ticks
			download file from "%{_link}%/main/experimental/%{_github}%/preset/%{_file}%.yml" to file "plugins/APIxMJ/%{_folder}%/preset/%{_file}%.yml"
	set {_list.3::*} to mj_editfiles_list("supervisor.config", "data.language.list")
	loop {_list.3::*}:
		download file from "%{_link}%/main/experimental/%{_github}%/language/%loop-value%.yml" to file "plugins/APIxMJ/%{_folder}%/language/%loop-value%.yml"
		download file from "%{_link}%/main/experimental/%{_github}%/help/%loop-value%.yml" to file "plugins/APIxMJ/%{_folder}%/help/%loop-value%.yml"
	stop
# ---------------------------------------------------------------------------------------------------------------------
# APIxMJ - Gui Control (Installer)
# =====================================================================================================================
# APIxMJ - Gui Control (Update)
# ---------------------------------------------------------------------------------------------------------------------
function mj_manager_menu_control_button_update(player: player, source: text, id: text, choice: number=1):
	set {_github} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.github_folder")
	set {_folder} to mj_editfiles("supervisor.data", "data.addon.%{_source}%.%{_id}%.folder")
	set {_check} to mj_editfiles_boolean("temp", "data.manager.addon.%{_source}%.%{_id}%.installed")
	if {_check} is true:
		set {_name} to mj_editfiles_gettext("supervisor", "data.button.update.name.%{_choice}%", {_player})
		set {_slot} to mj_editfiles_number("supervisor.data", "data.button.update.slot")
		if {_choice} is 2:
			set {_item} to minecart with furnace
		else if {_choice} is 3:
			set {_item} to minecart with chest
		else:
			set {_item} to minecart
		set {_count} to 2
		set {_version.1} to mj_editfiles("temp", "data.manager.addon.%{_source}%.%{_id}%.version")
		set {_version.2} to mj_editfiles_gettext("supervisor", "data.category.version.2", {_player}, {_version.1})
		set line {_count} of lore of {_item} to "%{_version.2}%"
		add 1 to {_count}
		set {_none} to mj_editfiles_gettext("supervisor", "data.none")
		set {_version.3} to mj_editfiles_gettext("supervisor", "data.category.version.3", {_player}, {_none})
		set line {_count} of lore of {_item} to "%{_version.3}%"
		if {_choice} is 3:
			set {_link} to mj_editfiles("github")
			set {_version.3} to text from "%{_link}%/main/experimental/%{_github}%/version.txt"
			set {_version.4} to mj_editfiles_gettext("supervisor", "data.category.version.3", {_player}, {_version.3})
			set line {_count} of lore of {_item} to "%{_version.4}%"
		set {_item} to mj_item_description({_player}, {_item}, "data.button.update")
		if {_choice} is 1:
			set {_item} to mj_item_button({_player}, {_item}, "left_click", "supervisor", "data.open.update_check")
			make a gui slot {_slot} of {_player} with {_item} named {_name} to run:
				mj_manager_menu_control_button_update_check({_player}, {_source}, {_id}, {_choice})
				stop
		else if {_choice} is 3:			
			set {_item} to mj_item_button({_player}, {_item}, "left_click", "supervisor", "data.open.update_now")
			make a gui slot {_slot} of {_player} with {_item} named {_name} to close:		
				mj_manager_menu_control_installer({_player}, {_source}, {_id}, "update")
				stop
		else:
			make a gui slot {_slot} of {_player} with {_item} named {_name} to run:
				stop
	stop
